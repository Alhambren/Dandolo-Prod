{
  "name": "@dandolo/agent-sdk",
  "version": "1.0.0",
  "description": "Superior TypeScript SDK for Dandolo AI - The best agent integration experience",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "browser": "dist/index.umd.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js",
      "browser": "./dist/index.umd.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "rollup -c --environment NODE_ENV:production",
    "build:dev": "rollup -c",
    "build:watch": "rollup -c --watch",
    "dev": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write \"src/**/*.{ts,js,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js,json}\"",
    "type-check": "tsc --noEmit",
    "clean": "rimraf dist coverage",
    "prepublishOnly": "npm run clean && npm run lint && npm run test && npm run build",
    "prepack": "npm run build",
    "release": "npm run prepublishOnly && npm publish",
    "docs": "typedoc src/index.ts --out docs --theme default"
  },
  "keywords": [
    "ai",
    "agents",
    "dandolo",
    "llm",
    "typescript",
    "openai-compatible",
    "streaming",
    "workflow",
    "context-aware"
  ],
  "author": "Dandolo.ai",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dandolo-ai/typescript-sdk"
  },
  "dependencies": {
    "axios": "^1.7.2",
    "eventemitter3": "^5.0.1",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.7",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "@typescript-eslint/parser": "^7.7.1",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "jest-extended": "^4.0.2",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "rollup": "^4.13.0",
    "rollup-plugin-dts": "^6.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "ts-jest": "^29.1.2",
    "typedoc": "^0.25.12",
    "typescript": "^5.4.5"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ]
}